{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Bing Wallpaper",
  "version": "0.1.0",
  "identifier": "com.wdbyte.bing.app",
  "build": {
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 0,
        "width": 0,
        "visible": false,
        "decorations": false,
        "transparent": true,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
